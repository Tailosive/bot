{
  "parser": "@typescript-eslint/parser",
  "plugins": ["@typescript-eslint"],
  "parserOptions": {
    "project": "./tsconfig.json",
    "sourceType": "module"
  },
  "rules": {
    "indent": [1, 2],
    "quotes": [1, "single"],
    "no-extra-semi": "error",
    "@typescript-eslint/no-unused-vars": 1,
    "keyword-spacing": 1,
    "space-before-function-paren": 1,
    "eqeqeq": 1,
    "space-infix-ops": 1,
    "comma-spacing": 1,
    "brace-style": 1,
    "curly": [1, "multi-or-nest"],
    "no-undef": 1,
    "no-multiple-empty-lines": 1,
    "one-var": [1, "never"],
    "operator-linebreak": 1,
    "no-cond-assign": 1,
    "block-spacing": 1,
    "comma-dangle": 1,
    "comma-style": 1,
    "dot-location": [1, "property"],
    "eol-last": 1,
    "func-call-spacing": 1,
    "key-spacing": 1,
    "new-cap": 1,
    "new-parens": 1,
    "accessor-pairs": 1,
    "constructor-super": 1,
    "no-caller": 1,
    "no-class-assign": 1,
    "no-const-assign": 1,
    "no-constant-condition": 1,
    "no-control-regex": 1,
    "no-delete-var": 1,
    "no-dupe-args": 1,
    "no-dupe-class-members": 1,
    "no-dupe-keys": 1,
    "no-duplicate-imports": 1,
    "no-empty-character-class": 1,
    "no-empty-pattern": 1,
    "no-eval": 1,
    "no-ex-assign": 1,
    "no-extend-native": 1,
    "no-extra-bind": 1,
    "no-extra-boolean-cast": 1,
    "no-extra-parens": [1, "functions"],
    "no-fallthrough": 1,
    "no-func-assign": 1,
    "no-global-assign": 1,
    "no-implied-eval": 1,
    "no-inner-declarations": 1,
    "no-irregular-whitespace": 1,
    "no-iterator": 1,
    "no-label-var": 1,
    "no-labels": 1,
    "no-lone-blocks": 1,
    "no-mixed-spaces-and-tabs": 1,
    "no-multi-str": 1,
    "no-new": 1,
    "no-new-func": 1,
    "no-new-require": 1,
    "no-new-wrappers": 1,
    "no-obj-calls": 1,
    "no-octal": 1,
    "no-octal-escape": 1,
    "no-path-concat": 1,
    "no-proto": 1,
    "no-redeclare": 1,
    "no-regex-spaces": 1,
    "no-return-assign": 1,
    "no-return-await": 1,
    "no-self-assign": 1,
    "no-self-compare": 1,
    "no-sequences": 1,
    "no-shadow-restricted-names": 1,
    "no-sparse-arrays": 1,
    "no-tabs": 1,
    "no-template-curly-in-string": 1,
    "no-this-before-super": 1,
    "no-throw-literal": 1,
    "no-trailing-spaces": 1,
    "no-undef-init": 1,
    "no-unmodified-loop-condition": 1,
    "no-unneeded-ternary": 1,
    "no-unreachable": 1,
    "no-unsafe-finally": 1,
    "no-unsafe-negation": 1,
    "no-useless-call": 1,
    "no-useless-computed-key": 1,
    "no-useless-constructor": 1,
    "no-useless-escape": 1,
    "no-useless-rename": 1,
    "no-whitespace-before-property": 1,
    "no-with": 1,
    "object-property-newline": 1,
    "padded-blocks": [1, "never"],
    "rest-spread-spacing": 1,
    "space-before-blocks": 1,
    "space-in-parens": 1,
    "space-unary-ops": 1,
    "spaced-comment": 1,
    "template-curly-spacing": 1,
    "use-isnan": 1,
    "valid-typeof": 1,
    "wrap-iife": 1,
    "yield-star-spacing": 1,
    "yoda": 1
  },
  "env": {
    "node": true,
    "es6": true
  }
}
